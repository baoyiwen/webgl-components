{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "typeRoots": ["./node_modules/@types", "./src"]
  },
  "include": ["vite.config.ts"],
  "exclude": [
    "assets",
    "component",
    "features",
    "hooks",
    "layout",
    "page",
    "settings",
    "store",
    "styles",
    "tests",
    "types",
    "utils"
  ]
}
